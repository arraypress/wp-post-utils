{
  "name": "arraypress/wp-post-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for working with posts and post operations",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-post-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\PostUtils\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-post-utils/issues",
    "source": "https://github.com/arraypress/wp-post-utils"
  },
  "keywords": [
    "wordpress",
    "posts",
    "wp",
    "arraypress",
    "post-meta",
    "content",
    "search",
    "options"
  ]
}